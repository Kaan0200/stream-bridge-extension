{
  "name": "stream-bridge-extension",
  "displayName": "Stream bridge extension",
  "version": "0.0.1",
  "description": "A plasmo browser extension that gives music stores a connection to listen to songs on a streaming service instead.",
  "author": "A.K.A",
  "license": "MIT",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@tidal-music/auth": "^1.4.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "scripting",
      "identity",
      "cookies"
    ],
    "host_permissions": [
      "https://www.beatport.com/*",
      "https://www.junodownload.com/*"
    ]
  }
}
